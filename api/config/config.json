{
    "version": "0.4",
    "sensor": {
        "ip": "localhost",
        "port": 3030
    },
    "server": {
        "ip": "0.0.0.0",
        "port": 8080,
        "static": {
            "serveStatic": true,
            "staticFolder": "./../static"
        },
        "rateLimit": {
            "enabled": true,
            "window": 10000,
            "max": 10
        },
        "tls": {
            "enabled": false,
            "key": "./certs/server.key",
            "cert": "./certs/server.cert"
        }
    },
    "log": {
        "enabled": true,
        "log": "./log/#.log"
    },
    "plugins": {
        "loadPlugins": true,
        "disabledPlugins": [
            "example.js"
        ]
    }
}