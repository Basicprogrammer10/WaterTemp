{
    "version": "0.2",
    "server": {
        "ip": "0.0.0.0",
        "port": 8080,
        "static": {
            "serveStatic": true,
            "staticFolder": "./../static"
        },
        "rateLimit": {
            "enabled": true,
            "window": 10000,
            "max": 10
        },
        "tls": {
            "enabled": false,
            "key": "./certs/server.key",
            "cert": "./certs/server.cert"
        }
    },
    "plugins": {
        "loadPlugins": true,
        "disabledPlugins": [
          "example.js"
        ]
      }
}